{
  "name": "py-lxml",
  "version": "6.0.0",
  "description": "Powerful and Pythonic XML processing library combining libxml2/libxslt with the ElementTree API.",
  "homepage": "https://lxml.de/",
  "dependencies": [
    {
      "name": "libxml2",
      "features": [
        "http"
      ]
    },
    {
      "name": "libxslt",
      "features": [
        "profiler"
      ]
    },
    {
      "name": "py-setuptools",
      "host": true
    },
    "python3",
    {
      "name": "vcpkg-python-scripts",
      "host": true
    }
  ]
}
